        {
            "parameters": {
                "jsCode": "// Get the original webhook data (not the Supabase response)\nconst webhookData = $('Webhook').first().json;\nconst photoUrls = webhookData.photo_urls || [];\n\n// Store the original entry data for later use\nconst entryData = {\n  date: webhookData.date || '',\n  name: webhookData.name || '',\n  area_name: webhookData.area_name || '',\n  extracted_text: webhookData.extracted_text || '',\n  timestamp: webhookData.timestamp || ''\n};\n\n// If no photos, return just the entry data\nif (photoUrls.length === 0) {\n  return [{ json: { ...entryData, photo_url: '' } }];\n}\n\n// Return one item per photo URL\nreturn photoUrls.map((url, i) => ({\n  json: {\n    photo_url: url,\n    photo_index: i,\n    ...entryData\n  }\n}));"
